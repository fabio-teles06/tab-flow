{
    "manifest_version": 3,
    "name": "Tab Flow",
    "version": "1.0",
    "description": "Troca automaticamente entre URLs de uma lista.",
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}